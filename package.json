{
    "name": "storage-json-editor-api",
    "version": "0.1.0",
    "description": "storage json editor api",
    "main": "main.js",
    "engines": {
        "node": ">=18.0",
        "npm": ">=8.0"
    },
    "scripts": {
        "test": "exit 0 && jest --config jestconfig.json --coverage",
        "build": "rm -rf dist && tsc",
        "start": "node dist/main.js",
        "dev": "npm run build && GOOGLE_APPLICATION_CREDENTIALS=key-file.json npm run start",
        "build:cloud": "gcloud builds submit --tag us-central1-docker.pkg.dev/zinovik-project/zinovik-repository/storage-json-editor-api --region=us-central1 --project zinovik-project",
        "deploy": "gcloud run deploy storage-json-editor-api --image us-central1-docker.pkg.dev/zinovik-project/zinovik-repository/storage-json-editor-api:latest --platform managed --region us-central1 --allow-unauthenticated --project zinovik-project"
    },
    "dependencies": {
        "@google-cloud/storage": "7.7.0",
        "@nestjs/common": "10.3.0",
        "@nestjs/core": "10.3.0",
        "@nestjs/platform-express": "10.3.0",
        "axios": "1.6.3",
        "google-auth-library": "9.4.1",
        "reflect-metadata": "0.1.14",
        "rxjs": "7.8.1"
    },
    "devDependencies": {
        "@types/body-parser": "1.19.5",
        "@types/jest": "29.5.11",
        "@types/node": "20.10.6",
        "jest": "29.7.0",
        "ts-jest": "29.1.1",
        "typemoq": "2.1.0",
        "typescript": "5.3.3"
    },
    "author": "Max Zinovik",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/zinovik/storage-json-editor-api.git"
    },
    "bugs": {
        "url": "https://github.com/zinovik/storage-json-editor-api/issues"
    },
    "homepage": "https://github.com/zinovik/storage-json-editor-api#readme"
}
